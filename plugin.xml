<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova-plugin-mathcalc" version="1.0.0">
   <name>MathCalc</name>
   <js-module name="MathCalc" src="www/MathCalc.js">
      <clobbers target="MathCalc" />
   </js-module>
   <platform name="android">
      <config-file parent="/*" target="res/xml/config.xml">
         <feature name="MathCalc">
            <param name="android-package" value="cordova.plugin.mathcalc.MathCalc" />
         </feature>
      </config-file>
      <config-file parent="/*" target="AndroidManifest.xml" />
      <source-file src="src/android/MathCalc.java" target-dir="src/cordova/plugin/mathcalc/MathCalc" />
   </platform>
</plugin>